<script>
  // JavaScript code to handle mobile menu and slide-over menu
  const mobileMenuButton = document.getElementById('mobile-menu-button');
  const mobileMenu = document.getElementById('mobile-menu');
  const closeMenuButton = document.getElementById('close-menu-button');
  const backdrop = document.getElementById('backdrop');

  function showMobileMenu() {
    mobileMenu.style.display = 'block';
  }

  function hideMobileMenu() {
    mobileMenu.style.display = 'none';
  }

  function showSlideOverMenu() {
    slideOverContainer.classList.add('overflow-hidden');
    slideOverMenu.style.transform = 'translateX(0)';
    backdrop.style.opacity = '0.5';
    backdrop.style.pointerEvents = 'auto';
  }

  function hideSlideOverMenu() {
    slideOverContainer.classList.remove('overflow-hidden');
    slideOverMenu.style.transform = 'translateX(100%)';
    backdrop.style.opacity = '0';
    backdrop.style.pointerEvents = 'none';
  }

  function toggleSlideOverMenu() {
    if (slideOverMenu.style.transform === 'translateX(0)') {
      hideSlideOverMenu();
    } else {
      showSlideOverMenu();
    }
  }

  backdrop.addEventListener('click', hideSlideOverMenu);

  mobileMenuButton.addEventListener('click', () => {
    showMobileMenu();
    showSlideOverMenu();
  });

  closeMenuButton.addEventListener('click', () => {
    hideMobileMenu();
    hideSlideOverMenu();
  });
</script>






